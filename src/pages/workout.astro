//style
import "../styles/pages/landing.scss";
// my library  animation
import "../styles/plugins/AEScrolling.scss";
import "../styles/pages/about.scss";
//data
import {workoutData} from "../data/data";
---

<Layout title="AtheleteZone | workout">
